<!DOCTYPE html>
<html>

<head>
	<title>Upcoming flights</title>
	<link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
	<h2>Upcoming flights</h2>
	<table>
		<thead>
			<tr>
				<th>Airline</th>
				<th>Airport</th>
				<th>Flight Number</th>
				<th>Departure Date</th>
			</tr>
		</thead>
		<tbody>
			<% flights.forEach(function(f) { %>
				<tr>
					<td><%= f.airline %></td>
					<td><%= f.airport %></td>
					<td><%= f.flightNo %></td>
					<td><%= f.departs %></td>
				</tr>
				<% }) %>
		</tbody>
	</table>
	<a href="/">Back</a>
</body>

</html>